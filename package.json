		"@xmtp/content-type-transaction-reference": "^2.0.2",
		"@xmtp/content-type-wallet-send-calls": "^2.0.0",
		"@xmtp/node-sdk": "^4.0.3",
		"ai": "4.3.19",
		"dotenv": "^17.2.0",
		"uint8arrays": "^5.1.0",
		"viem": "^2.31.7"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.1",
		"@bitte-ai/types": "^0.8.0",
		"@types/node": "^22.16.4",
		"typescript": "^5.8.3"
	},
