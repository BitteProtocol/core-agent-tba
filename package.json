{
  "name": "bitte-xmtp-client",
  "version": "0.1.0",
  "description": "XMTP Client for Bitte Agent",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun revoke && bun src/index.ts",
    "gen-keys": "bun src/scripts/generate-keys",
    "revoke": "bun src/scripts/revoke-installations",
    "wipe:db": "rm -rf .data",
    "lint": "biome check",
    "format": "biome check --write --unsafe"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.24",
    "@ai-sdk/provider-utils": "2.2.8",
    "@xmtp/content-type-group-updated": "^2.0.2",
    "@xmtp/content-type-reaction": "^2.0.2",
    "@xmtp/content-type-reply": "^2.0.2",
    "@xmtp/content-type-text": "^2.0.2",
    "@xmtp/content-type-transaction-reference": "^2.0.2",
    "@xmtp/content-type-wallet-send-calls": "^2.0.0",
    "@xmtp/node-sdk": "^4.1.0",
    "ai": "4.3.19",
    "dotenv": "^17.2.1",
    "uint8arrays": "^5.1.0",
    "viem": "^2.31.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@bitte-ai/types": "^0.8.0",
    "@types/node": "^22.18.0",
    "typescript": "^5.8.3"
  },
  "author": "Bitte",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  }
}
