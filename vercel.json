{
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/api/index"
    },
    {
      "source": "/(.+)",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/index.ts": {
      "maxDuration": 300
    }
  },
  "crons": [
    { "path": "/api", "schedule": "*/5 * * * *" }
  ]
}